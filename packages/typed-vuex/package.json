{
  "name": "typed-vuex",
  "version": "0.3.0",
  "description": "A typed store accessor for Vuex.",
  "keywords": [
    "vuex",
    "nuxt",
    "typescript",
    "vue",
    "vuejs",
    "nuxtjs",
    "nuxt-module"
  ],
  "repository": "danielroe/typed-vuex",
  "license": "MIT",
  "author": {
    "name": "Daniel Roe",
    "email": "daniel@roe.dev",
    "url": "https://github.com/danielroe"
  },
  "main": "lib/index.js",
  "module": "lib/index.es.js",
  "types": "lib/index.d.ts",
  "files": [
    "lib/**/*",
    "lib/index.d.ts",
    "!**/*.map"
  ],
  "scripts": {
    "build": "yarn clean && yarn compile",
    "clean": "rm -rf lib",
    "compile": "siroc build",
    "test:types": "yarn tsd"
  },
  "devDependencies": {
    "@vue/test-utils": "^1.0.0",
    "tsd": "^0.19.1",
    "vue": "^2.6.14",
    "vuex": "^3.6.2"
  },
  "peerDependencies": {
    "vuex": "^3.6.2"
  },
  "tsd": {
    "directory": "test/tsd",
    "compilerOptions": {
      "rootDir": "."
    }
  }
}
